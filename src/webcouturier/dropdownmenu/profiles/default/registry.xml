      <value key="css">
        <element>++plone++dropdown/dropdown.less</element>
      </value>
  </records>

  <records prefix="plone.resources/webcouturier-dropdownmenu-bundle"
           interface="Products.CMFPlone.interfaces.IResourceRegistry">
